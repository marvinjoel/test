<odoo>
  <record id="falabella_params" model="ir.config_parameter">
    <field name="key">falabella.user</field>
    <field name="value">sistemasghlperu@gmail.com</field>
  </record>
  <record id="falabella_token" model="ir.config_parameter">
    <field name="key">falabella.token</field>
    <field name="value">TOKEN---------------------</field>
  </record>

  <record id="ir_cron_sync_falabella_products" model="ir.cron">
    <field name="name">Sincronizar Productos con Falabella</field>
    <field name="model_id" ref="model_falabella_cron"/>
    <field name="state">code</field>
    <field name="code">model.run_price_name_sync()</field>
    <field name="interval_number">5</field>
    <field name="interval_type">minutes</field>
    <field name="numbercall">-1</field>
    <field name="active">True</field>
  </record>

</odoo>